{
  "name": "yaba",
  "productName": "yaba",
  "version": "2.0.0",
  "description": "Yaba: Yet Another Budgeting App lets you budget with your finances with tags instead of categories.",
  "main": ".webpack/main",
  "scripts": {
    "prestart": "npm run build",
    "start": "electron-forge start",
    "build": "tsc && ng build",
    "package": "npm run build && electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "ng lint && eslint --ext .ts,.tsx .",
    "test": "ng test",
    "e2e": "ng e2e"
  },
  "keywords": [
    "yaba",
    "budgeting",
    "finance",
    "productivity",
    "organization"
  ],
  "author": {
    "name": "Markizano Draconus",
    "email": "markizano@markizano.net"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markizano/yaba.git"
  },
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/markizano/yaba/issues"
  },
  "homepage": "https://github.com/markizano/yaba#readme",
  "dependencies": {
    "@angular/animations": "20.1.4",
    "@angular/cli": "^20.1.4",
    "@angular/common": "^20.1.4",
    "@angular/compiler": "^20.1.4",
    "@angular/core": "^20.1.4",
    "@angular/forms": "^20.1.4",
    "@angular/material": "^20.1.4",
    "@angular/platform-browser": "^20.1.4",
    "@angular/platform-browser-dynamic": "^20.1.4",
    "@angular/router": "^20.1.4",
    "@ng-select/ng-select": "^20.0.1",
    "@types/uuid": "^10.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "file-saver": "^2.0.5",
    "google": "^2.1.0",
    "jszip": "^3.10.1",
    "ngx-papaparse": "^8.0.0",
    "papaparse": "^5.4.1",
    "path-browserify": "^1.0.1",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.0",
    "zone.js": "^0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.1.4",
    "@electron-forge/cli": "^7.8.2",
    "@electron-forge/maker-deb": "^7.8.2",
    "@electron-forge/maker-rpm": "^7.8.2",
    "@electron-forge/maker-squirrel": "^7.8.2",
    "@electron-forge/maker-zip": "^7.8.2",
    "@electron-forge/plugin-webpack": "^7.8.2",
    "@types/file-saver": "^2.0.7",
    "@types/jasmine": "~4.3.0",
    "@types/papaparse": "^5.3.14",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "angular-google-charts": "^16.1.0",
    "css-loader": "^6.11.0",
    "electron": "^37.2.5",
    "electron-squirrel-startup": "^1.0.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.5.3",
    "fork-ts-checker-webpack-plugin": "^7.3.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "node-loader": "^2.1.0",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "webpack-stream": "^7.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^app/(.*)$": "<rootDir>/$1"
    },
    "rootDir": "web/app",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "app/**/*.(t|j)s"
    ],
    "coverageDirectory": "coverage",
    "testEnvironment": "node"
  }
}
